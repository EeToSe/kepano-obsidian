filters:
  or:
    - linksTo(file.file, this.file.path)
    - contains(property.created, this.file.name)
    - contains(property.last, this.file.name)
views:
  - type: table
    name: Day
    order:
      - file.name
    sort:
      - column: property.created
        direction: ASC
  - type: table
    name: Daily note
    order:
      - file.name
    sort:
      - column: property.created
        direction: ASC
